{
  "buildCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GOOGLE_SHEET_URL": "@google_sheet_url"
  }
}
